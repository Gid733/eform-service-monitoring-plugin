  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microting.eFormApi.BasePn">
      <HintPath>..\..\eFormApi.BasePn\eFormApi.BasePn\bin\Debug\netcoreapp2.2\Microting.eFormApi.BasePn.dll</HintPath>
    </Reference>
    <Reference Include="Microting.EformMonitoringBase">
